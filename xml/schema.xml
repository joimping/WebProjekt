            <xs:element name="Automarke" type="xs:string">
            <xs:element name="Autobezeichnung" type="xs:string">
            <xs:element name="Technische_Details">
            <xs:complexType>
                <xs:sequence>

                    <xs:element name="Leistung" type="xs:string">
                    <xs:element name="max_Drehmomemt" type="xs:string">
                    
                </xs:sequence>
            </complexType>
        </xs:sequence>
    </complexType>
</xs:element>

